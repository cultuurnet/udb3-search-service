{
    "name": "cultuurnet/udb3-search-service",
    "description": "Silex application for indexing UDB3 JSON-LD documents and providing a search api.",
    "type": "project",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Publiq vzw",
            "email": "info@publiq.be"
        }
    ],
    "require": {
        "php": "^7.1",
        "2dotstwice/silex-feature-toggles-provider": "~0.1",
        "broadway/broadway": "~0.10",
        "cakephp/chronos": "^1.1",
        "cultuurnet/auth": "~1.2",
        "cultuurnet/broadway-amqp": "~0.1",
        "cultuurnet/culturefeed-php": "~1.9.2",
        "cultuurnet/deserializer": "~0.1",
        "cultuurnet/geocoding": "~0.1",
        "cultuurnet/hydra": "~0.1",
        "cultuurnet/silex-amqp": "~0.1",
        "cultuurnet/udb3-api-guard": "~0.1",
        "cultuurnet/udb3-http-foundation": "~0.1",
        "cultuurnet/valueobjects": "~3.0",
        "danielstjules/stringy": "~1.9",
        "deralex/yaml-config-service-provider": "~1.0",
        "doctrine/cache": "~1.8",
        "elasticsearch/elasticsearch": "~5.2",
        "guzzlehttp/guzzle": "^6.3",
        "guzzlehttp/psr7": "^1.4",
        "hassankhan/config": "^2.1",
        "jdesrosiers/silex-cors-provider": "~0.1.4",
        "knplabs/console-service-provider": "~1.0",
        "league/container": "^3.3",
        "league/route": "^4.3",
        "monolog/monolog": "~1.11",
        "ongr/elasticsearch-dsl": "~5.0.1",
        "predis/predis": "~1.0",
        "silex/silex": "~1.3",
        "slim/psr7": "^0.5.0",
        "symfony/finder": "~3.2",
        "symfony/http-foundation": "~2.7",
        "zendframework/zend-httphandlerrunner": "^1.1"
    },
    "require-dev": {
        "escapestudios/symfony2-coding-standard": "~2.9",
        "phing/phing": "~2.14",
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/phpunit": "^7.5",
        "squizlabs/php_codesniffer": "~2.5.1"
    },
    "autoload": {
        "psr-4": {
            "CultuurNet\\UDB3\\Search\\": "src/",
            "CultuurNet\\UDB3\\SearchService\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CultuurNet\\UDB3\\Search\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "config": {
        "preferred-install": {
            "cultuurnet/*": "source",
            "2dotstwice/*": "source",
            "*": "dist"
        },
        "sort-packages": true
    }
}

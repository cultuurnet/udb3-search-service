{
    "name": "cultuurnet/udb3-search-service",
    "description": "Silex application for indexing UDB3 JSON-LD documents and providing a search api.",
    "type": "project",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Bert Ramakers",
            "email": "bert@2dotstwice.be"
        }
    ],
    "require": {
        "php": "^7.1",
        "2dotstwice/silex-feature-toggles-provider": "~0.1",
        "cultuurnet/culturefeed-php": "~1.9.2",
        "cultuurnet/geocoding": "~0.1",
        "cultuurnet/silex-amqp": "~0.1",
        "cultuurnet/udb3": "~0.1",
        "cultuurnet/udb3-api-guard": "~0.1",
        "cultuurnet/udb3-http-foundation": "~0.1",
        "cultuurnet/udb3-search": "~0.1",
        "cultuurnet/udb3-search-elasticsearch": "~0.1",
        "cultuurnet/udb3-search-http": "~0.1",
        "deralex/yaml-config-service-provider": "~1.0",
        "doctrine/cache": "~1.8",
        "jdesrosiers/silex-cors-provider": "~0.1.4",
        "knplabs/console-service-provider": "~1.0",
        "monolog/monolog": "~1.11",
        "predis/predis": "~1.0",
        "silex/silex": "~1.3"
    },
    "require-dev": {
        "escapestudios/symfony2-coding-standard": "~2.9",
        "phing/phing": "~2.14",
        "phpunit/phpunit": "~5.7",
        "satooshi/php-coveralls": "~1.0",
        "squizlabs/php_codesniffer": "~2.5.1"
    },
    "autoload": {
        "psr-4": {
            "CultuurNet\\UDB3\\SearchService\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CultuurNet\\UDB3\\SearchService\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "config": {
        "preferred-install": {
            "cultuurnet/*": "source",
            "2dotstwice/*": "source",
            "*": "dist"
        },
        "sort-packages": true
    }
}
